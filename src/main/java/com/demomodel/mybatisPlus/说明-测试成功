CREATE TABLE `t_student` (
	`sid` INT(11) NOT NULL AUTO_INCREMENT,
	`sname` VARCHAR(255) NULL DEFAULT '0',
	`sex` CHAR(2) NULL DEFAULT '0',
	PRIMARY KEY (`sid`)
)
COMMENT='学员表'
COLLATE='utf8mb4_0900_ai_ci'
ENGINE=InnoDB
